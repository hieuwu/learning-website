{{#section 'css'}}
<link rel="stylesheet" href="../../public/stylesheets/home.css">
{{/section}}
<div class="row justify-content-center">
    <div class="col-sm-8">
        <div class="homeslider">
            <div id="carouselExampleControls"  data-interval="2000" class="carousel slide" data-ride="carousel">
                <div class="carousel-inner">
                    <div class="carousel-item active">
                        <img class="d-block w-100" src="https://i.imgur.com/sCsZDfp.jpg" alt="First slide">
                        <div class="carousel-caption d-md-block">
                            <h1>Khóa học nổi bật nhất trong tuần</h1>
                        </div>
                    </div>
                    <div class="carousel-item">
                        <img class="d-block w-100" src="https://i.imgur.com/lbwfIoA.jpg" alt="Second slide">
                        <div class="carousel-caption d-md-block">
                            <h1>Khóa học nổi bật nhất trong tuần</h1>
                        </div>
                    </div>
                    <div class="carousel-item">
                        <img class="d-block w-100" src="https://i.imgur.com/vHNHZCC.jpg" alt="Third slide">
                        <div class="carousel-caption d-md-block">
                            <h1>Khóa học nổi bật nhất trong tuần</h1>
                        </div>
                    </div>
                </div>
                <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
                    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                    <span class="sr-only">Previous</span>
                </a>
                <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
                    <span class="carousel-control-next-icon" aria-hidden="true"></span>
                    <span class="sr-only">Next</span>
                </a>
            </div>
        </div>
    </div>
</div>

<div>
    <div class="row col-md-12">
        <div class="col-md-1 carousel-title"></div>
        <div class="col-md-10 carousel-title">Latest course
            <div>
                <hr/>
            </div>
        </div>
        <div class="col-md-1"></div>
    </div>
    <div>
        <div class="col-md-12">
            <div id="mostViewCarousel" class="carousel slide" data-ride="carousel" data-interval="0">
                <!-- Carousel indicators -->
                <ol class="carousel-indicators">
                    <li data-target="#mostViewCarousel" data-slide-to="0" class="active"></li>
                    <li data-target="#mostViewCarousel" data-slide-to="1"></li>
                </ol>
                <!-- Wrapper for carousel items -->
                <div class="carousel-inner">
                    <div class="carousel-item active">
                        <div class="row justify-content-center">
                            {{#each latestPage1}}
                                <div class="col-sm-2">
                                    <div class="thumb-wrapper">
                                        <div class="img-box">
                                            <img src='./public/images/course/{{this.IdCourse}}/thumb.jpg'
                                                 class="img-fluid" alt="">
                                        </div>
                                        <div class="thumb-content course-item">
                                           <div class="two-line"> <h5>{{this.nameCourse}}</h5></div>
                                            <div class="two-line"> {{{this.title}}}</div>
                                            <div><h6>{{this.NameCategory}}</h6></div>
                                            <div> <h6>{{this.FullName}}</h6></div>
                                            <div class="item-price"><strike>${{this.Price}}</strike>
                                                <span>${{this.SaleCost}}</span></div>
                                            <div class="star-rating">
                                                <ul class="list-inline">
                                                    <li class="list-inline-item"><i class="fa fa-star"></i></li>
                                                    <li class="list-inline-item"><i class="fa fa-star"></i></li>
                                                    <li class="list-inline-item"><i class="fa fa-star"></i></li>
                                                    <li class="list-inline-item"><i class="fa fa-star"></i></li>
                                                    <li class="list-inline-item"><i class="fa fa-star-o"></i></li>
                                                </ul>
                                            </div>
                                            <a href="#" class="btn btn-primary">View course</a>
                                        </div>
                                    </div>
                                </div>
                            {{/each}}
                        </div>
                    </div>
                    <div class="carousel-item">
                        <div class="row justify-content-center">
                            {{#each latestPage2}}
                                 <div class="col-sm-2">
                                    <div class="thumb-wrapper">
                                        <div class="img-box">
                                            <img src='./public/images/course/{{this.IdCourse}}/thumb.jpg'
                                                 class="img-fluid" alt="">
                                        </div>
                                        <div class="thumb-content course-item">
                                           <div class="two-line"> <h5>{{this.nameCourse}}</h5></div>
                                            <div class="two-line"> {{{this.title}}}</div>
                                            <div><h6>{{this.NameCategory}}</h6></div>
                                            <div> <h6>{{this.FullName}}</h6></div>
                                            <div class="item-price"><strike>${{this.Price}}</strike>
                                                <span>${{this.SaleCost}}</span></div>
                                            <div class="star-rating">
                                                <ul class="list-inline">
                                                    <li class="list-inline-item"><i class="fa fa-star"></i></li>
                                                    <li class="list-inline-item"><i class="fa fa-star"></i></li>
                                                    <li class="list-inline-item"><i class="fa fa-star"></i></li>
                                                    <li class="list-inline-item"><i class="fa fa-star"></i></li>
                                                    <li class="list-inline-item"><i class="fa fa-star-o"></i></li>
                                                </ul>
                                            </div>
                                            <a href="#" class="btn btn-primary">View course</a>
                                        </div>
                                    </div>
                                </div>
                            {{/each}}
                        </div>
                    </div>

                </div>
                <a class="carousel-control-prev" href="#mostViewCarousel" data-slide="prev">
                    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                    <span class="sr-only">Previous</span> </a>
                <a class="carousel-control-next" href="#mostViewCarousel" data-slide="next">
                    <span class="carousel-control-next-icon" aria-hidden="true"></span>
                    <span class="sr-only">Next</span> </a>
            </div>
        </div>
    </div>
</div>
</div>
