{{#section 'css'}}
<link rel="stylesheet" href="/public/stylesheets/bootstrap.min.css">
<link rel="stylesheet" href="/public/stylesheets/home.css">
<link rel="stylesheet" href="/public/stylesheets/jsRapStar.css" />
{{/section}}
{{#section 'js'}}
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="/public/javascript/jsRapStar.js"></script>
<script>$('.ratingStar').jsRapStar({ enabled: false, colorFront: "yellow" });</script>
{{/section}}
<div class="row justify-content-center">
    <div class="col-sm-8">
        <div class="homeslider">
            <div id="carouselExampleControls" data-interval="2000" class="carousel slide" data-ride="carousel">
                <div class="carousel-inner">
                    {{#each listOfHighlightCourses}}
                    <div class="carousel-item {{#if this.isActive}} active {{/if}}">
                        <a href="/course/detail/{{this.IdCourse}}">
                            <img class="d-block w-100" src='/public/images/course/{{this.IdCourse}}/thumb.jpg'
                                alt="First slide">
                            <div class="carousel-caption d-md-block">
                                <h1 class="bg-text">{{this.nameCourse}}</h1>
                            </div>
                        </a>

                    </div>
                    {{/each}}
                </div>
                <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
                    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                    <span class="sr-only">Previous</span>
                </a>
                <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
                    <span class="carousel-control-next-icon" aria-hidden="true"></span>
                    <span class="sr-only">Next</span>
                </a>
            </div>
        </div>
    </div>
</div>
<div>
    <div class="row col-md-12">
        <div class="col-md-1 carousel-title"></div>
        <div class="col-md-10 carousel-title">Latest course
            <div>
                <hr />
            </div>
        </div>
        <div class="col-md-1"></div>
    </div>
    <div>
        <div class="col-md-12">
            <div id="mostViewCarousel" class="carousel slide" data-ride="carousel" data-interval="0">
                <!-- Carousel indicators -->
                <ol class="carousel-indicators">
                    <li data-target="#mostViewCarousel" data-slide-to="0" class="active"></li>
                    <li data-target="#mostViewCarousel" data-slide-to="1"></li>
                </ol>
                <!-- Wrapper for carousel items -->
                <div class="carousel-inner">
                    <div class="carousel-item active">
                        <div class="row justify-content-center">
                            {{#each latestPage1}}
                            <div class="col-sm-2">
                                <div class="thumb-wrapper">
                                    <div class="img-box">
                                        <img src='/public/images/course/{{this.IdCourse}}/thumb.jpg' class="img-fluid"
                                            alt="">
                                    </div>
                                    <div class="thumb-content course-item">
                                        <div class="two-line">
                                            <h5>{{this.nameCourse}}</h5>
                                        </div>
                                        <div class="two-line"> {{{this.title}}}</div>
                                        <div>
                                            <h6>{{this.NameCategory}}</h6>
                                        </div>
                                        <div>
                                            <h6>{{this.FullName}}</h6>
                                        </div>
                                        <div class="item-price"><strike>${{this.Price}}</strike>
                                            <span>${{this.SaleCost}}</span>
                                        </div>
                                        <div class="d-flex align-items-center">
                                            <p class="mb-0 mr-1 mt-1 avgRating">{{this.avgRate}}</p>
                                            <div class="ratingStar" value={{this.avgRate}}></div>
                                            <p class="mb-0">({{this.numRating}} Ratings)</p>
                                        </div>
                                        <a href="/course/detail/{{this.IdCourse}}" class="btn btn-primary">View
                                            course</a>
                                    </div>
                                </div>
                            </div>
                            {{/each}}
                        </div>
                    </div>
                    <div class="carousel-item">
                        <div class="row justify-content-center">
                            {{#each latestPage2}}
                            <div class="col-sm-2">
                                <div class="thumb-wrapper">
                                    <div class="img-box">
                                        <img src='/public/images/course/{{this.IdCourse}}/thumb.jpg' class="img-fluid"
                                            alt="">
                                    </div>
                                    <div class="thumb-content course-item">
                                        <div class="two-line">
                                            <h5>{{this.nameCourse}}</h5>
                                        </div>
                                        <div class="two-line"> {{{this.title}}}</div>
                                        <div>
                                            <h6>{{this.NameCategory}}</h6>
                                        </div>
                                        <div>
                                            <h6>{{this.FullName}}</h6>
                                        </div>
                                        <div class="item-price"><strike>${{this.Price}}</strike>
                                            <span>${{this.SaleCost}}</span>
                                        </div>
                                        <div class="d-flex align-items-center">
                                            <p class="mb-0 mr-1 mt-1 avgRating">{{this.avgRate}}</p>
                                            <div class="ratingStar" value={{this.avgRate}}></div>
                                            <p class="mb-0">({{this.numRating}} Ratings)</p>
                                        </div>
                                        <a href="/course/detail/{{this.IdCourse}}" class="btn btn-primary">View
                                            course</a>
                                    </div>
                                </div>
                            </div>
                            {{/each}}
                        </div>
                    </div>

                </div>
                <a class="carousel-control-prev" href="#mostViewCarousel" data-slide="prev">
                    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                    <span class="sr-only">Previous</span> </a>
                <a class="carousel-control-next" href="#mostViewCarousel" data-slide="next">
                    <span class="carousel-control-next-icon" aria-hidden="true"></span>
                    <span class="sr-only">Next</span> </a>
            </div>
        </div>
    </div>
</div>
</div>
